<Window x:Class="MarcinS.Projekt.BrowseDestinations"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarcinS.Projekt"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="BrowseDestinations" Height="450" Width="800">
    <Canvas>
        <Image x:Name="image" HorizontalAlignment="Left" Height="310" Width="450" Canvas.Left="25" Canvas.Top="25" Source="{Binding}"/>
        <DataGrid x:Name="destinations" Height="310" Width="275" Canvas.Right="25" Canvas.Top="25" ItemsSource="{Binding}" SelectedCellsChanged="destinations_SelectedCellsChanged"/>
    </Canvas>
</Window>
