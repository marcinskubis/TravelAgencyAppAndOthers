<Window x:Class="MarcinS.Projekt.ProjectMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarcinS.Projekt"
        mc:Ignorable="d"
        Title="ProjectMainWindow" Height="450" Width="800">
    <Canvas>
        <Button x:Name="AddTrip" Width="300" Height="70" Canvas.Top="60" Canvas.Left="70" Content="Dodaj wycieczkę" FontSize="30" Click="AddTrip_Click"/>
        <Button x:Name="Trip" Width="300" Height="70" Canvas.Top="60" Canvas.Right="70" Content="Przeglądaj wycieczki" FontSize="30" Click="Trip_Click"/>
        <Button x:Name="Destination" Width="300" Height="70" Canvas.Top="200" Canvas.Left="70" Content="Dodaj kierunek" FontSize="30"/>
    </Canvas>
</Window>
