<Window x:Class="MarcinS.Projekt.BrowseTrips"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MarcinS.Projekt"
        mc:Ignorable="d"
        Title="BrowseTrips" Height="450" Width="800">
    <Canvas>
        <DataGrid x:Name="tripsTable" HorizontalAlignment="Left" Height="374" VerticalAlignment="Top" Width="500" ItemsSource="{Binding}" Canvas.Left="34" Canvas.Top="30"/>
        <Button x:Name="loadData" Content="Pokaż" HorizontalAlignment="Left" VerticalAlignment="Top" Height="63" Width="183" Click="loadData_Click" Canvas.Left="572" Canvas.Top="46"/>
        <Button x:Name="deleteData" Content="Usuń" HorizontalAlignment="Left" VerticalAlignment="Top" Height="63" Width="183" Click="deleteData_Click" Canvas.Left="572" Canvas.Top="151"/>
    </Canvas>
</Window>
